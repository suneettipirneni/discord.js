{
	"name": "@discordjs/validators",
	"version": "0.1.0",
	"description": "",
	"scripts": {
		"test": "vitest run",
		"build": "tsup",
		"build:docs": "tsc -p tsconfig.docs.json",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src __tests__ --ext .mjs,.js,.ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src __tests__ --ext .mjs,.js,.ts --fix --format=pretty",
		"docs": "yarn build:docs && api-extractor run --local",
		"prepack": "yarn build && yarn lint",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/validators/*'",
		"release": "cliff-jumper"
	},
	"exports": {
		"./components/button": {
			"import": "./dist/button.mjs",
			"require": "./dist/button.js",
			"types": "./dist/button.d.ts"
		},
		"./components/selectMenu": {
			"import": "./dist/selectMenu.mjs",
			"require": "./dist/selectMenu.js",
			"types": "./dist/selectMenu.d.ts"
		},
		"./messages/embed": {
			"import": "./dist/embed.mjs",
			"require": "./dist/embed.js",
			"types": "./dist/embed.d.ts"
		}
	},
	"directories": {
		"lib": "src",
		"test": "__tests__"
	},
	"files": [
		"dist"
	],
	"contributors": [
		"Crawl <icrawltogo@gmail.com>",
		"SpaceEEC <spaceeec@yahoo.com>",
		"Vlad Frangu <kingdgrizzle@gmail.com>",
		"Aura Rom√°n <kyradiscord@gmail.com>"
	],
	"license": "Apache-2.0",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/discordjs/discord.js.git"
	},
	"bugs": {
		"url": "https://github.com/discordjs/discord.js/issues"
	},
	"homepage": "https://discord.js.org",
	"devDependencies": {
		"@favware/cliff-jumper": "^1.10.0",
		"@microsoft/api-extractor": "^7.33.7",
		"@types/node": "16.18.11",
		"@vitest/coverage-c8": "^0.29.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.31.0",
		"eslint-config-neon": "^0.1.40",
		"eslint-formatter-pretty": "^4.1.0",
		"prettier": "^2.8.2",
		"tsup": "^6.7.0",
		"typescript": "^5.0.2",
		"vitest": "^0.29.7"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@sapphire/shapeshift": "^3.8.1",
		"discord-api-types": "^0.37.37"
	}
}
